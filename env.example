# Environment Variables for Time Marketplace
# Copy this file to .env and fill in your values

# Sepolia RPC URL (optional - defaults to public RPC)
SEPOLIA_RPC_URL=https://sepolia.drpc.org

# Private key for contract deployment (optional - only needed for deployment)
PRIVATE_KEY=your_private_key_here

# Contract address (will be filled automatically after deployment)
CONTRACT_ADDRESS=

# Network chain ID
CHAIN_ID=11155111